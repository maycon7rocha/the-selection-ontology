<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.iff.edu.br/schemas/selection#"
     xml:base="http://www.iff.edu.br/schemas/selection#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:data-view="http://www.w3.org/2003/g/data-view#">
    <owl:Ontology rdf:about="http://www.iff.edu.br/schemas/selection#"/>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
  
    <!-- http://www.iff.edu.br/schemas/selection#class -->

    <owl:ObjectProperty rdf:about="http://www.iff.edu.br/schemas/selection#class">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    </owl:ObjectProperty>
    
    <!-- http://www.iff.edu.br/schemas/selection#condition -->

    <owl:ObjectProperty rdf:about="http://www.iff.edu.br/schemas/selection#condition">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.iff.edu.br/schemas/selection#Condition"/>
    </owl:ObjectProperty>
    
    <!-- http://www.iff.edu.br/schemas/selection#criteriaClass -->

    <owl:ObjectProperty rdf:about="http://www.iff.edu.br/schemas/selection#criteriaClass">
        <rdfs:domain rdf:resource="http://www.iff.edu.br/schemas/selection#Conditional"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.iff.edu.br/schemas/selection#condition"/>
            <rdf:Description rdf:about="http://www.iff.edu.br/schemas/selection#class"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    
    <!-- http://www.iff.edu.br/schemas/selection#desire -->

    <owl:ObjectProperty rdf:about="http://www.iff.edu.br/schemas/selection#desire">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.iff.edu.br/schemas/selection#Selection"/>
        <rdfs:range rdf:resource="http://www.iff.edu.br/schemas/selection#Desire"/>
    </owl:ObjectProperty>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.iff.edu.br/schemas/selection#importanceLevel -->

    <owl:DatatypeProperty rdf:about="http://www.iff.edu.br/schemas/selection#importanceLevel">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.iff.edu.br/schemas/selection#Desire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.iff.edu.br/schemas/selection#AndCondition -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#AndCondition">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#BinaryCompoundCondition"/>
        <owl:disjointWith rdf:resource="http://www.iff.edu.br/schemas/selection#OrCondition"/>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#BinaryCompoundCondition -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#BinaryCompoundCondition">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#CompoundCondition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.iff.edu.br/schemas/selection#condition"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.iff.edu.br/schemas/selection#UnaryCompoundCondition"/>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#CompoundCondition -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#CompoundCondition">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#Condition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.iff.edu.br/schemas/selection#condition"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.iff.edu.br/schemas/selection#SimpleCondition"/>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#Condition -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#Condition">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.iff.edu.br/schemas/selection#CompoundCondition"/>
                    <rdf:Description rdf:about="http://www.iff.edu.br/schemas/selection#SimpleCondition"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.iff.edu.br/schemas/selection#class"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#Conditional -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#Conditional">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.iff.edu.br/schemas/selection#condition"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#Desire -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#Desire">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#Conditional"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.iff.edu.br/schemas/selection#importanceLevel"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.iff.edu.br/schemas/selection#Selection"/>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#NotCondition -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#NotCondition">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#UnaryCompoundCondition"/>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#OrCondition -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#OrCondition">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#BinaryCompoundCondition"/>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#Selection -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#Selection">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#Conditional"/>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#SimpleCondition -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#SimpleCondition">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#Condition"/>
    </owl:Class>
    
    <!-- http://www.iff.edu.br/schemas/selection#UnaryCompoundCondition -->

    <owl:Class rdf:about="http://www.iff.edu.br/schemas/selection#UnaryCompoundCondition">
        <rdfs:subClassOf rdf:resource="http://www.iff.edu.br/schemas/selection#CompoundCondition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.iff.edu.br/schemas/selection#condition"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <!-- http://www.w3.org/2000/01/rdf-schema#Class -->

    <owl:Class rdf:about="http://www.w3.org/2000/01/rdf-schema#Class"/>
</rdf:RDF>